   <SourceLine></SourceLine>
   <SourceLine>skipComma mp, pos</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = matchDouble(mp, pos, tmpX)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>skipComma mp, pos</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>result = matchDouble(mp, pos, tmpY)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>skipComma mp, pos</SourceLine>
   <SourceLine></SourceLine>
