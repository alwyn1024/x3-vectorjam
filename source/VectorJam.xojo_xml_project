  <IsShared>0</IsShared>
  <ItemParams>x As Integer, y As Integer, width As Integer, height As Integer</ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>DrawPicture</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function DrawPicture(image As Picture, x As Integer, y As Integer, w1 As Integer = -10000, h1 As Integer = -10000, sx As Integer = 0, sy As Integer = 0, w2 As Integer = -10000, h2 As Integer = -10000) As X3Core.X3Model</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee}â„¢ initiative.</SourceLine>
   <SourceLine>' All X3 code is donated to the public domain and can be used freely in commercial and non-commercial applications.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim model As new X3Core.X3Model</SourceLine>
   <SourceLine>Dim poly As X3Core.X3Polygon</SourceLine>
   <SourceLine>Dim x1 As Double</SourceLine>
   <SourceLine>Dim y1 As Double</SourceLine>
   <SourceLine>Dim x2 As Double</SourceLine>
   <SourceLine>Dim y2 As Double</SourceLine>
   <SourceLine>Dim midX As Double</SourceLine>
   <SourceLine>Dim midY As Double</SourceLine>
   <SourceLine>Dim u1 As Double</SourceLine>
   <SourceLine>Dim v1 As Double</SourceLine>
   <SourceLine>Dim u2 As Double</SourceLine>
   <SourceLine>Dim v2 As Double</SourceLine>
   <SourceLine>Dim w As Double</SourceLine>
   <SourceLine>Dim h As Double</SourceLine>
   <SourceLine>Dim iWidth As Double</SourceLine>
   <SourceLine>Dim iHeight As Double</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// do integer to double conversion once</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>iWidth = image.Width</SourceLine>
   <SourceLine>iHeight = image.Height</SourceLine>
   <SourceLine>w = Width</SourceLine>
   <SourceLine>h = Height</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// calculate xy coordinates</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' mapX(x)</SourceLine>
   <SourceLine>x1 = x - (w / 2)</SourceLine>
   <SourceLine>x1 = x1 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' mapY(y)</SourceLine>
   <SourceLine>y1 = (h / 2) - y</SourceLine>
   <SourceLine>y1 = y1 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if w1 &gt; 0 Then</SourceLine>
   <SourceLine>' mapX(x + w1)</SourceLine>
   <SourceLine>x2 = x + w1 - (w / 2)</SourceLine>
   <SourceLine>x2 = x2 / h</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>' mapX(x + texture.Width)</SourceLine>
   <SourceLine>x2 = x + iWidth - (w / 2)</SourceLine>
   <SourceLine>x2 = x2 / h</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>if h1 &gt; 0 Then</SourceLine>
   <SourceLine>' mapY(y + h1)</SourceLine>
   <SourceLine>y2 = (h / 2) - y - h1</SourceLine>
   <SourceLine>y2 = y2 / h</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>' mapY(y + texture.Height)</SourceLine>
   <SourceLine>y2 = (h / 2) - y - iHeight</SourceLine>
   <SourceLine>y2 = y2 / h</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>midX = (x2 - x1) / 2</SourceLine>
   <SourceLine>midY = (y2 - y1) / 2</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Position.X = x1 + midX</SourceLine>
   <SourceLine>model.Position.Y = y1 + midY</SourceLine>
   <SourceLine>model.Position.Z = 0</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>x1 = x1 - model.Position.X</SourceLine>
   <SourceLine>y1 = y1 - model.Position.Y</SourceLine>
   <SourceLine>x2 = x2 - model.Position.X</SourceLine>
   <SourceLine>y2 = y2 - model.Position.Y</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// calculate uv coordinates</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>u1 = sx / image.Width</SourceLine>
   <SourceLine>if w2 &gt; 0 then</SourceLine>
   <SourceLine>u2 = (sx + w2) / iWidth</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>if w1 &gt; 0 Then</SourceLine>
   <SourceLine>u2 = (sx + w1) / iWidth</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>u2 = 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>v1 =  sy / image.Width</SourceLine>
   <SourceLine>if h2 &gt; 0 then</SourceLine>
   <SourceLine>v2 = (sy + h2) / iHeight</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>if h1 &gt; 0 Then</SourceLine>
   <SourceLine>v2 = (sy + h1) / iHeight</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>v2 = 1</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add texture (created from image)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Texture.Append new X3Core.X3Texture(image)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add uv-coordinates</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.UVMap.Append new X3Core.X3UVCoordinate(u1, v1)</SourceLine>
   <SourceLine>model.UVMap.Append new X3Core.X3UVCoordinate(u1, v2)</SourceLine>
   <SourceLine>model.UVMap.Append new X3Core.X3UVCoordinate(u2, v2)</SourceLine>
   <SourceLine>model.UVMap.Append new X3Core.X3UVCoordinate(u2, v1)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add vertices </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x1, y1, 0)</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x2, y1, 0)</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x1, y2, 0)</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x2, y2, 0)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// create polygon</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly = new X3Core.X3Polygon()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly.VIndex.Append 0</SourceLine>
   <SourceLine>poly.VIndex.Append 2</SourceLine>
   <SourceLine>poly.VIndex.Append 3</SourceLine>
   <SourceLine>poly.VIndex.Append 1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly.TIndex = 0 // point to image texture</SourceLine>
   <SourceLine>poly.UVIndex.Append 0</SourceLine>
   <SourceLine>poly.UVIndex.Append 1</SourceLine>
   <SourceLine>poly.UVIndex.Append 2</SourceLine>
   <SourceLine>poly.UVIndex.Append 3</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal() // update polygon normal</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Scene.Model.Append model // add model to scene</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return model</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>image As Picture, x As Integer, y As Integer, w1 As Integer = -10000, h1 As Integer = -10000, sx As Integer = 0, sy As Integer = 0, w2 As Integer = -10000, h2 As Integer = -10000</ItemParams>
  <ItemResult>X3Core.X3Model</ItemResult>
 </Method>
 <Method>
  <ItemName>FillRect</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function FillRect(x As Integer, y As Integer, width As Integer, height As Integer) As X3Core.X3Model</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee}â„¢ initiative.</SourceLine>
   <SourceLine>' All X3 code is donated to the public domain and can be used freely in commercial and non-commercial applications.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim model As new X3Core.X3Model</SourceLine>
   <SourceLine>Dim poly As new X3Core.X3Polygon</SourceLine>
   <SourceLine>Dim x1 As Double</SourceLine>
   <SourceLine>Dim y1 As Double</SourceLine>
   <SourceLine>Dim x2 As Double</SourceLine>
   <SourceLine>Dim y2 As Double</SourceLine>
   <SourceLine>Dim midX As Double</SourceLine>
   <SourceLine>Dim midY As Double</SourceLine>
   <SourceLine>Dim w As Double</SourceLine>
   <SourceLine>Dim h As Double</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// do integer to double conversion once</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>w = Width</SourceLine>
   <SourceLine>h = Height</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add color</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.ColorPal.Append new X3Core.X3Color(ForeColor)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add vertices</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' mapX(x)</SourceLine>
   <SourceLine>x1 = x - (w / 2)</SourceLine>
   <SourceLine>x1 = x1 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' mapY(y)</SourceLine>
   <SourceLine>y1 = (h / 2) - y</SourceLine>
   <SourceLine>y1 = y1 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' mapX(x + width)</SourceLine>
   <SourceLine>x2 = (x + width) - (w / 2)</SourceLine>
   <SourceLine>x2 = x2 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' mapY(y +height)</SourceLine>
   <SourceLine>y2 = (h / 2) - (y +height)</SourceLine>
   <SourceLine>y2 = y2 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>'x1 = MapX(x)</SourceLine>
   <SourceLine>'y1 = MapY(y)</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'x2 = MapX(x + width)</SourceLine>
   <SourceLine>'y2 = MapY(y +height)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>midX = (x2 - x1) / 2</SourceLine>
   <SourceLine>midY = (y2 - y1) / 2</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Position.X = x1 + midX</SourceLine>
   <SourceLine>model.Position.Y = y1 + midY</SourceLine>
   <SourceLine>model.Position.Z = 0</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>x1 = x1 - model.Position.X</SourceLine>
   <SourceLine>y1 = y1 - model.Position.Y</SourceLine>
   <SourceLine>x2 = x2 - model.Position.X</SourceLine>
   <SourceLine>y2 = y2 - model.Position.Y</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x1, y1, 0)</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x1, y2, 0)</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x2, y2, 0)</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x2, y1, 0)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// create polygon</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly.VIndex.Append 0</SourceLine>
   <SourceLine>poly.VIndex.Append 1</SourceLine>
   <SourceLine>poly.VIndex.Append 2</SourceLine>
   <SourceLine>poly.VIndex.Append 3</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly.CIndex = 0 // point to color</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// set UV coordinates</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly.UVIndex.Append 0</SourceLine>
   <SourceLine>poly.UVIndex.Append 1</SourceLine>
   <SourceLine>poly.UVIndex.Append 2</SourceLine>
   <SourceLine>poly.UVIndex.Append 3</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal() // update polygon normal</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Scene.Model.Append model // add model to scene</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return model</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>x As Integer, y As Integer, width As Integer, height As Integer</ItemParams>
  <ItemResult>X3Core.X3Model</ItemResult>
 </Method>
 <Method>
  <ItemName>FillPolygon</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub FillPolygon(points() As Integer)</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee}â„¢ initiative.</SourceLine>
   <SourceLine>' All X3 code is donated to the public domain and can be used freely in commercial and non-commercial applications.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim model As X3Core.X3Model</SourceLine>
   <SourceLine>model = FillPolygon(points)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>points() As Integer</ItemParams>
  <ItemResult></ItemResult>
 </Method>
 <Method>
  <ItemName>FillPolygon</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function FillPolygon(points() As Integer) As X3Core.X3Model</SourceLine>
   <SourceLine>' www.Xojo3D.com</SourceLine>
   <SourceLine>' X3 is a {Zoclee}â„¢ initiative.</SourceLine>
   <SourceLine>' All X3 code is donated to the public domain and can be used freely in commercial and non-commercial applications.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Dim model As new X3Core.X3Model</SourceLine>
   <SourceLine>Dim poly As new X3Core.X3Polygon</SourceLine>
   <SourceLine>Dim x As Double</SourceLine>
   <SourceLine>Dim y As Double</SourceLine>
   <SourceLine>Dim i As Integer</SourceLine>
   <SourceLine>Dim pointCount As Integer</SourceLine>
   <SourceLine>Dim xSum As Double</SourceLine>
   <SourceLine>Dim ySum As Double</SourceLine>
   <SourceLine>Dim midX As Double</SourceLine>
   <SourceLine>Dim midY As Double</SourceLine>
   <SourceLine>Dim totVerts As Integer</SourceLine>
   <SourceLine>Dim w As Double</SourceLine>
   <SourceLine>Dim h As Double</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// do integer to double conversion once</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>w = Width</SourceLine>
   <SourceLine>h = Height</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add color</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.ColorPal.Append new X3Core.X3Color(ForeColor)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add vertices</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>pointCount = (points.Ubound \ 2)</SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>xSum = 0</SourceLine>
   <SourceLine>ySum = 0</SourceLine>
   <SourceLine>while i &lt; pointCount</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>x = points(1 + i * 2) - (w / 2)</SourceLine>
   <SourceLine>x = x / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>y = (h / 2) - points(2 + i * 2)</SourceLine>
   <SourceLine>y = y / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>xSum = xSum + x</SourceLine>
   <SourceLine>ySum = ySum + y</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x, y, 0)</SourceLine>
   <SourceLine>poly.VIndex.Append i</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// center polygon</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>totVerts = model.Vertex.Ubound + 1</SourceLine>
   <SourceLine>midX = xSum / totVerts</SourceLine>
   <SourceLine>midY = ySum / totVerts</SourceLine>
   <SourceLine>for i = 0 to model.Vertex.Ubound</SourceLine>
   <SourceLine>model.Vertex(i).X = model.Vertex(i).X - midX</SourceLine>
   <SourceLine>model.Vertex(i).Y = model.Vertex(i).Y - midY</SourceLine>
   <SourceLine>next  i</SourceLine>
   <SourceLine>model.Position.X = midX</SourceLine>
   <SourceLine>model.Position.Y = midY</SourceLine>
   <SourceLine>model.Position.Z = 0</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly.CIndex = 0 // point to color</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal() // update polygon normal</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' if the polygon is facing away from us, we need need to reverse the vertices</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if model.Normal(poly.NIndex).Z &lt; 0 then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' first remove the existing vertices</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>i = 0</SourceLine>
   <SourceLine>while i &lt; pointCount</SourceLine>
   <SourceLine>model.Vertex.Remove(model.Vertex.Ubound)</SourceLine>
   <SourceLine>i = i + 1</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>' add the vertices in reversed order</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>i = pointCount - 1</SourceLine>
   <SourceLine>xSum = 0</SourceLine>
   <SourceLine>ySum = 0</SourceLine>
   <SourceLine>while i &gt;= 0</SourceLine>
   <SourceLine>x = points(1 + i * 2) - (w / 2)</SourceLine>
   <SourceLine>x = x / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>y = (h / 2) - points(2 + i * 2)</SourceLine>
   <SourceLine>y = y / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>xSum = xSum + x</SourceLine>
   <SourceLine>ySum = ySum + y</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x, y, 0)</SourceLine>
   <SourceLine>i = i - 1</SourceLine>
   <SourceLine>wend</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// center polygon</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>midX = xSum / totVerts</SourceLine>
   <SourceLine>midY = ySum / totVerts</SourceLine>
   <SourceLine>for i = 0 to model.Vertex.Ubound</SourceLine>
   <SourceLine>model.Vertex(i).X = model.Vertex(i).X - midX</SourceLine>
   <SourceLine>model.Vertex(i).Y = model.Vertex(i).Y - midY</SourceLine>
   <SourceLine>next  i</SourceLine>
   <SourceLine>model.Position.X = midX</SourceLine>
   <SourceLine>model.Position.Y = midY</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly.CalculateNormal() ' calculate the normal of the polygon</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Scene.Model.Append model // add model to scene</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return model</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
