 <Method>
  <ItemName>DrawString</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function DrawString(str As String, x As Integer, y As Integer, width As Integer = 0, condense As Boolean = False) As X3Core.X3Model</SourceLine>
   <SourceLine>Dim model As new X3Core.X3Model</SourceLine>
   <SourceLine>Dim poly As new X3Core.X3Polygon</SourceLine>
   <SourceLine>Dim image As Picture</SourceLine>
   <SourceLine>Dim mask As Picture</SourceLine>
   <SourceLine>Dim x1 As Double</SourceLine>
   <SourceLine>Dim y1 As Double</SourceLine>
   <SourceLine>Dim x2 As Double</SourceLine>
   <SourceLine>Dim y2 As Double</SourceLine>
   <SourceLine>Dim midX As Double</SourceLine>
   <SourceLine>Dim midY As Double</SourceLine>
   <SourceLine>Dim w As Double</SourceLine>
   <SourceLine>Dim h As Double</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// do integer to double conversion once</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>w = Me.Width</SourceLine>
   <SourceLine>h = Me.Height</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// init picture calculator </SourceLine>
   <SourceLine>calcPic.Graphics.Bold = Bold</SourceLine>
   <SourceLine>calcPic.Graphics.Italic = Italic</SourceLine>
   <SourceLine>calcPic.Graphics.TextFont = TextFont</SourceLine>
   <SourceLine>calcPic.Graphics.TextSize = TextSize</SourceLine>
   <SourceLine>calcPic.Graphics.TextUnit = TextUnit</SourceLine>
   <SourceLine>calcPic.Graphics.Underline = Underline</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if width &gt; 0 then</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if calcPic.Graphics.StringWidth(str) &gt; width then</SourceLine>
   <SourceLine>if condense then</SourceLine>
   <SourceLine>image = new Picture(width, calcPic.Graphics.StringHeight("Ay", width), 24)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>image = new Picture(width, calcPic.Graphics.StringHeight(str, width), 24)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>image = new Picture(calcPic.Graphics.StringWidth(str), calcPic.Graphics.StringHeight(str, width), 24)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>image = new Picture(calcPic.Graphics.StringWidth(str), calcPic.Graphics.StringHeight(str, width), 24)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>mask = new Picture(image.Width, image.Height, 24)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>image.Graphics.Bold = Bold</SourceLine>
   <SourceLine>image.Graphics.Italic = Italic</SourceLine>
   <SourceLine>image.Graphics.TextFont = TextFont</SourceLine>
   <SourceLine>image.Graphics.TextSize = TextSize</SourceLine>
   <SourceLine>image.Graphics.TextUnit = TextUnit</SourceLine>
   <SourceLine>image.Graphics.Underline = Underline</SourceLine>
   <SourceLine>image.Graphics.ForeColor = ForeColor</SourceLine>
   <SourceLine>image.Graphics.DrawString str, 0, calcPic.Graphics.TextAscent, width, condense</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>mask.Graphics.Bold = Bold</SourceLine>
   <SourceLine>mask.Graphics.Italic = Italic</SourceLine>
   <SourceLine>mask.Graphics.TextFont = TextFont</SourceLine>
   <SourceLine>mask.Graphics.TextSize = TextSize</SourceLine>
   <SourceLine>mask.Graphics.TextUnit = TextUnit</SourceLine>
   <SourceLine>mask.Graphics.Underline = Underline</SourceLine>
   <SourceLine>mask.Graphics.ForeColor = rgb(0, 0, 0, ForeColor.Alpha)</SourceLine>
   <SourceLine>mask.Graphics.DrawString str, 0, calcPic.Graphics.TextAscent, width, condense</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>image.Mask = mask</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add vertices</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>x1 = x - (w / 2)</SourceLine>
   <SourceLine>x1 = x1 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>y1 = (h / 2) - (y - calcPic.Graphics.TextAscent)</SourceLine>
   <SourceLine>y1 = y1 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>x2 = (x + image.Width) - (w / 2)</SourceLine>
   <SourceLine>x2 = x2 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>y2 = (h / 2) - (y - calcPic.Graphics.TextAscent + image.Height)</SourceLine>
   <SourceLine>y2 = y2 / h</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>midX = (x2 - x1) / 2</SourceLine>
   <SourceLine>midY = (y2 - y1) / 2</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Position.X = x1 + midX</SourceLine>
   <SourceLine>model.Position.Y = y1 + midY</SourceLine>
   <SourceLine>model.Position.Z = 0</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>x1 = x1 - model.Position.X</SourceLine>
   <SourceLine>y1 = y1 - model.Position.Y</SourceLine>
   <SourceLine>x2 = x2 - model.Position.X</SourceLine>
   <SourceLine>y2 = y2 - model.Position.Y</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x1, y1, 0)</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x1, y2, 0)</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x2, y2, 0)</SourceLine>
   <SourceLine>model.Vertex.Append new X3Core.X3Vector(x2, y1, 0)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add uv-coordinates</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.UVMap.Append new X3Core.X3UVCoordinate(0, 0)</SourceLine>
   <SourceLine>model.UVMap.Append new X3Core.X3UVCoordinate(0, 1)</SourceLine>
   <SourceLine>model.UVMap.Append new X3Core.X3UVCoordinate(1, 1)</SourceLine>
   <SourceLine>model.UVMap.Append new X3Core.X3UVCoordinate(1, 0)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// add texture (created from image)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.Texture.Append new X3Core.X3Texture(image)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// create polygon</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly.VIndex.Append 0</SourceLine>
   <SourceLine>poly.VIndex.Append 1</SourceLine>
   <SourceLine>poly.VIndex.Append 2</SourceLine>
   <SourceLine>poly.VIndex.Append 3</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>poly.TIndex = 0 // point to image texture</SourceLine>
   <SourceLine>poly.UVIndex.Append 0</SourceLine>
   <SourceLine>poly.UVIndex.Append 1</SourceLine>
   <SourceLine>poly.UVIndex.Append 2</SourceLine>
   <SourceLine>poly.UVIndex.Append 3</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>model.AppendPolygon poly</SourceLine>
   <SourceLine>poly.CalculateNormal() // update polygon normal</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Scene.Model.Append model // add model to scene</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return model</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>str As String, x As Integer, y As Integer, width As Integer = 0, condense As Boolean = False</ItemParams>
  <ItemResult>X3Core.X3Model</ItemResult>
